{
    "name": "@sap/ux-ui5-fe-mockserver-middleware",
    "version": "1.5.1",
    "description": "SAP Fiori tools - UI5 middleware for the Fiori elements mock server",
    "publisher": "SAPSE",
    "license": "SEE LICENSE IN LICENSE",
    "private": false,
    "main": "dist/index.js",
    "module": "src/index.ts",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "ui5-dist",
        "README.md",
        "package.json",
        "ui5.yaml",
        "ui5-serve.yaml"
    ],
    "scripts": {
        "pre-commit": "lint-staged --quiet",
        "clean": "rimraf ./build ./dist ./ui5-dist",
        "clean-dev": "rimraf ./build ./dist",
        "copy-dts": "copyfiles -a -u 1 \"build/**/*.d.ts\" dist",
        "copy-js": "copyfiles -a -u 1 \"build/**/*.js\" dist",
        "copy-dts-and-js": "npm-run-all -p copy-dts copy-js",
        "build": "npm-run-all clean build:compile webpack ui5-build copy-dts",
        "build:compile": "tsc --project ./tsconfig.build.json",
        "dev": "yarn clean-dev && tsc -project ./tsconfig.dev.json --watch",
        "lint": "eslint src --ext .ts --quiet",
        "lint:fix": "eslint src --ext .ts --fix --quiet",
        "format": "prettier src --write --ignore-path ../../.prettierignore",
        "test": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --silent",
        "test:update": "jest --silent -u",
        "webpack": "webpack",
        "ui5-build": "webpack --config webpack-ui5.config.js && node createUI5File"
    },
    "dependencies": {
        "@ui5/logger": "^2.0.1",
        "balanced-match": "1.0.0",
        "body-parser": "1.19.0",
        "chevrotain": "9.1.0",
        "chokidar": "3.5.2",
        "graceful-fs": "4.2.9",
        "lodash.clonedeep": "4.5.0",
        "query-string": "6.8.3",
        "router": "^1.3.5"
    },
    "peerDependencies": {
        "@sap/cds-compiler": "^2.2.6"
    },
    "devDependencies": {
        "@sap-ux/annotation-converter": "0.3.37",
        "@sap-ux/edmx-parser": "0.3.37",
        "@sap-ux/vocabularies-types": "0.3.37",
        "@sap/cds-compiler": "^2.2.6",
        "@types/body-parser": "1.17.1",
        "@types/express": "4.17.1",
        "@types/jest": "^26.0.24",
        "@types/lodash.clonedeep": "4.5.6",
        "assert": "^2.0.0",
        "browserify-zlib": "0.2.0",
        "copyfiles": "2.4.1",
        "express": "4.17.1",
        "fs-extra": "^6.0.1",
        "jest": "^27.0.6",
        "jest-environment-node": "^27.0.6",
        "jest-sonar-reporter": "^2.0.0",
        "nise": "^4.0.4",
        "node-fetch": "2.6.7",
        "npm-run-all": "4.1.5",
        "null-loader": "4.0.1",
        "os-browserify": "0.3.0",
        "path-browserify": "^1.0.1",
        "rimraf": "^3.0.2",
        "source-map-loader": "3.0.0",
        "stream-browserify": "3.0.0",
        "stream-http": "3.2.0",
        "ts-jest": "27.0.3",
        "typescript": "4.3.5",
        "url": "^0.11.0",
        "webpack": "5.58.1",
        "webpack-cli": "4.9.0",
        "webpack-node-externals": "3.0.0"
    },
    "jestSonar": {
        "reportPath": "reports/test/unit",
        "reportFile": "test-report.xml"
    }
}
